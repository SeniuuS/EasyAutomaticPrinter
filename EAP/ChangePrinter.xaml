<Window x:Class="EAP.ChangePrinter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EAP"
        mc:Ignorable="d"
        Title="Change Printer" Height="300" Width="300">
    <DockPanel Margin="10">
        <StackPanel DockPanel.Dock="Bottom">
            <Button Name="btnSelect" Width="50"  Click="btnSelect_Click">Select</Button>
        </StackPanel>
        <ListBox Name="printers" SelectionChanged="printers_SelectionChanged"></ListBox>
    </DockPanel>
</Window>
